{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "hackex-app/Dockerfile"
  },
  "deploy": {
    "startCommand": "php-fpm -D && php artisan queue:work --daemon & nginx -g 'daemon off;'",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}
